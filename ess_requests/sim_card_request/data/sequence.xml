<odoo>
    <data noupdate="1">
        <record id="sequence_request_sim_card" model="ir.sequence">
           <field name="name">Request SIM card</field>
           <field name="code">sim.card</field>
           <field name="active">TRUE</field>
           <field name="prefix">REQ-NO-</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
        </record>


    </data>

    <data noupdate="1">
        <record id="sequence_request_sim_card_change" model="ir.sequence">
           <field name="name">Request SIM card Rate Plan</field>
           <field name="code">sim.card.change</field>
           <field name="active">TRUE</field>
           <field name="prefix">REQ-NO-</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
        </record>


    </data>
</odoo>